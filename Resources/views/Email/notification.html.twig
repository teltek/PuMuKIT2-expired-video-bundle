
<h2>{{ subject }}</h2>

<br/>
{% if type == "removeOwner" %}

    <p>{% trans %}This video is out of date: {% endtrans %} {{ mmobj.title }}</p>
    <p>{% trans %}The owner of this video has been deleted.{% endtrans %}</p>

{% elseif type == "expired" %}
    <p>{% trans %}This video will be expire soon{% endtrans %}: {{ mmobj.title }}</p>
    <p>{% trans %}Access this url to renew the video: {% endtrans %}</p>
    <ul>
        <li><a href="{{ url('homepage') }}admin/expired/video/renew/{{ token }}">{{ url('homepage') }}admin/expired/video/renew/{{ token }}/{{ mmobj.id }}</a></li>
    </ul>
{% endif %}

<br/>
<br/>
{{ sender_name }}
