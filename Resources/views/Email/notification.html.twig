<h2>{{ subject|trans }}</h2>

<br/>

<p>{% trans %}To renew all your videos access this url:{% endtrans %}</p>
<ul>
    <li>
        <a href="{{ url('homepage') }}admin/expired/video/all/renew/{{ data.all }}/">
            {{ url('homepage') }}admin/expired/video/all/renew/{{ data.all }}/
        </a>
    </li>
</ul>

{% for video in data.videos %}
    <p>{% trans %}This video will be expire soon{% endtrans %}: {{ video.title }}</p>
    <p>{% trans %}Access this url to renew the video: {% endtrans %}</p>
    <ul>
        <li>
            <a href="{{ url('homepage') }}admin/expired/video/renew/{{ video.token }}">
                {{ url('homepage') }}admin/expired/video/renew/{{ video.token }}/
            </a>
        </li>
    </ul>
    <br/>
{% endfor %}

<br/>
<br/>
{{ sender_name }}
